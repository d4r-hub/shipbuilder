# Vault Configuration
VAULT_ADDR=https://vault.example.com
VAULT_USERNAME=your-ldap-username
VAULT_PASSWORD=your-ldap-password
VAULT_PATH=auth/ldap/login
VAULT_TOKEN=your-vault-token
TOKEN_FILE=${HOME}/.vault-token

# Artifactory Configuration
ARTIFACTORY_URL=https://artifactory.example.com
ARTIFACTORY_USER=your-artifactory-user
ARTIFACTORY_PASS=your-artifactory-pass

# AWS Configuration (Open Environment)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_DEFAULT_REGION=us-west-2
AWS_VPC_ID=vpc-xxxxxxxx
AWS_SUBNET_ID=subnet-xxxxxxxx

# vSphere Configuration (Closed Environment)
VSPHERE_USER=your-vsphere-user
VSPHERE_PASSWORD=your-vsphere-password
VSPHERE_SERVER=vsphere.example.com
VSPHERE_DATACENTER=your-datacenter
VSPHERE_CLUSTER=your-cluster

# Image Builder Configuration
COMPOSER_URL=https://image-builder.example.com/api/image-builder/v1
COMPOSER_TOKEN=your-composer-token

# Build Configuration
PACKER_LOG=0
PACKER_LOG_PATH=packer.log
ANSIBLE_DEBUG=false 